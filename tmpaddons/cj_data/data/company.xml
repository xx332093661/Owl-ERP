    <!--新增四川省川酒集团信息科技有限公司-->
    <data noupdate="1">
        <record id="res_partner_company_cj" model="res.partner">
            <field name="name">四川省川酒集团信息科技有限公司</field>
            <field eval="0" name="customer"/>
            <field eval="0" name="supplier"/>
            <field name="is_company">1</field>
            <field eval="1" name="active"/>
        </record>

        <record id="res_company_cj" model="res.company">
            <field name="currency_id" ref="base.CNY"/>
            <field name="partner_id" ref="res_partner_company_cj"/>
            <field name="parent_id" ref="base.main_company"/>
            <field name="code">CJXX</field>
            <field name="name">四川省川酒集团信息科技有限公司</field>
        </record>

        <!--admin允许的公司增加四川省川酒集团信息科技有限公司-->
        <record id="base.user_admin" model="res.users">
            <field name="company_ids" eval="[(4, ref('res_company_cj'))]"/>
        </record>
    </data>

