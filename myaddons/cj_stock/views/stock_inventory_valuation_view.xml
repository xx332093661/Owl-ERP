    <data>
        <record id="view_stock_inventory_valuation_move_tree" model="ir.ui.view">
            <field name="name">stock.inventory.valuation.move.tree</field>
            <field name="model">stock.inventory.valuation.move</field>
            <field name="arch" type="xml">
                <tree create="0" delete="0" edit="0">
                    <field name="company_id" invisible="context.get('hide_company_id', False)"/>
                    <field name="warehouse_id"/>
                    <field name="type"/>
                    <field name="type_id"/>
                    <field name="product_id"/>
                    <field name="date"/>
                    <field name="product_qty"/>
                    <!--<field name="price_unit"/>-->
                    <field name="unit_cost"/>
                    <field name="qty_available"/>
                    <field name="stock_cost"/>
                    <field name="stock_value"/>
                    <!--<field name="move_id"/>-->
                </tree>
            </field>
        </record>
        <record id="view_stock_inventory_valuation_move_filter" model="ir.ui.view">
            <field name="name">stock.inventory.valuation.move.filter</field>
            <field name="model">stock.inventory.valuation.move</field>
            <field name="arch" type="xml">
                <search>
                    <group expand="0" string="Group By">
                        <filter string="公司" name="company" domain="[]" context="{'group_by':'company_id'}"/>
                        <filter string="产品" name="product" domain="[]" context="{'group_by':'product_id'}"/>
                    </group>
                </search>
            </field>
        </record>
    </data>


</odoo>