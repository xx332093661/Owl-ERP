<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="view_import_delivery_order_package_wizard_form" model="ir.ui.view">
            <field name="name">import.delivery.order.package.wizard form</field>
            <field name="model">import.delivery.order.package.wizard</field>
            <field name="arch" type="xml">
                <form string="导入物流单成本包装物向导">
                    <sheet>
                        <group>
                            <label for="import_file"/>
                            <div>
                                <field name="import_file" nolabel="1"/>
                            </div>
                            <field name="overlay"/>
                        </group>
                        <group>
                            <label for="id" string="模板下载"/>
                            <div>
                                <a href="/cj_stock/static/template/import_delivery_order_package.xlsx" target="_blank">下载</a>
                            </div>
                        </group>
                    </sheet>
                    <footer>
                        <button string='导入' name="button_ok" type="object" class="btn-primary"/>
                        <button string='导入并确认' name="button_ok" type="object" class="btn-secondary" context="{'confirm': 1}"/>
                        <button string="取消" class="btn-secondary" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

        <act_window
            id="action_import_delivery_order_package_wizard"
            name="导入物流单成本包装物"
            res_model="import.delivery.order.package.wizard"
            src_model="delivery.order"
            view_mode="form"
            multi="True"
            target="new"
            key2="client_action_multi"
            groups="stock.group_stock_user"
        />
    </data>

</odoo>



